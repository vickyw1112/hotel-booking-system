package domain;

import java.util.LinkedList;

public class Pair {
	private LinkedList<Room> rooms;
	private Hotel hotel;
	private String name;
	
	public struct(Hotel hotel, LinkedList<Room> rooms) {
		this.rooms = rooms;
		this.hotel = hotel;
	}
	
	
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}


	/**
	 * @return the bookings
	 */
	public LinkedList<Room> getRooms() {
		return rooms;
	}

	/**
	 * @return the hotel
	 */
	public Hotel getHotel() {
		return hotel;
	}
	
	
}
